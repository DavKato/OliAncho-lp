{
  "name": "oliancho-lp",
  "description": "oliancho-lp",
  "version": "0.0.1",
  "license": "MIT",
  "browserslist": [
    "defaults",
    "not ie<=11"
  ],
  "scripts": {
    "dev": "sapper dev",
    "build": "NODE_ENV=production sapper build --legacy",
    "export": "NODE_ENV=production sapper export --legacy",
    "ex:firebase": "NODE_ENV=production sapper export --legacy && rm -rf firebase/public && cp -r __sapper__/export firebase/public",
    "tc:dev": "testcafe",
    "start": "node __sapper__/build",
    "test": "rollup -c rollup.test.config.js && riteway test/bundle-tests.js | tap-nirvana"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "polka": "0.5.2",
    "sirv": "^0.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@rollup/plugin-multi-entry": "^3.0.0",
    "@rollup/plugin-replace": "^2.3.1",
    "autoprefixer": "^9.7.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "riteway": "^6.1.1",
    "rollup": "^1.31.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.2.0",
    "sapper": "^0.27.9",
    "svelte": "^3.18.1",
    "svelte-i-pack": "^0.2.8",
    "svelte-preprocess": "^3.4.0",
    "tap-nirvana": "^1.1.0",
    "testcafe": "^1.8.1"
  }
}
